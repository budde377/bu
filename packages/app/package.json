{
  "description": "Thang.io",
  "scripts": {
    "postinstall": "scripts/postinstall.js",
    "build:docker": "docker build --build-arg VERSION=$VERSION -t budde377/thang-app:latest .",
    "publish:docker": "docker push budde377/thang-app:latest",
    "build:server": "NODE_ENV=production webpack  --progress --profile --no-colors --config webpack.config.server.js",
    "build": "NODE_ENV=production webpack  --progress --profile --no-colors --config webpack.config.js",
    "start": "node dist/server",
    "start:server": "NODE_ENV=development webpack --watch --config webpack.config.server.js & nodemon -w dist/server dist/server",
    "start:client": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js  --hot",
    "test:flow": "flow .",
    "test:lint": "eslint .",
    "test": "npm run test:flow && npm run test:lint",
    "dev": "npm run start:client & npm run start:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/budde377/thang.git"
  },
  "keywords": [
    "thang"
  ],
  "author": "Christian Budde Christensen <budde377@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/budde377/thang/issues"
  },
  "homepage": "https://github.com/budde377/thang#readme",
