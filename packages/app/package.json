    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "js-yaml": "^3.11.0",
    "koa": "^2.5.0",
    "koa-compress": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-logger": "^3.2.0",
    "koa-session": "^5.8.1",
